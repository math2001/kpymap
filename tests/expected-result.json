[
    {
        "keys": ["ctrl+k", "ctrl+c"],
        "command": "show_panel",
        "args": {
            "toggle": true,
            "panel": "console"
        }
    },
    {
        "keys": ["ctrl+k", "ctrl+d"],
        "command": "console_cleanr"
    },
    {
        "keys": ["ctrl+k", "ctrl+m"],
        "command": "toggle_minimap"
    },
    {
        "keys": ["ctrl+k", "ctrl+s"],
        "command": "toggle_side_bar"
    },
    {
        "keys": ["ctrl+k", "ctrl+o"],
        "command": "toggle_show_open_files"
    },
    {
        "keys": ["alt+f"],
        "command": "fold_python_functions",
        "context": [
            {"key": "selector", "operand": "source.python", "match_all": true}
        ]
    },
    {
        "keys": ["alt+e"],
        "command": "insert",
        "args": {
            "characters": "enumerate($1)"
        },
        "context": [
            {"key": "selector", "operand": "source.python", "match_all": true}
        ]
    },
    {
        "keys": ["ctrl+o"],
        "command": "markdown_preview",
        "args": {
            "parser": "github",
            "target": "browser"
        },
        "context": [
            {"key": "selector", "operand": "text.html.markdown"}
        ]
    },
    {
        "keys": [".", "."],
        "command": "insert",
        "args": {
            "characters": "self."
        },
        "context": [
            {"key": "selector", "operand": "source.python - comment - string"}
        ]
    },
    {
        "keys": ["f8"],
        "command": "chain",
        "args": {
            "commands": [
                [
                    "save"
                ],
                [
                    "close_window"
                ]
            ]
        },
        "context": [
            {"key": "selector", "operand": "text.git-commit-message"}
        ]
    },
    {
        "keys": ["<", "-"],
        "command": "insert",
        "args": {
            "characters": "← "
        }
    },
    {
        "keys": ["-", ">"],
        "command": "insert",
        "args": {
            "characters": "→ "
        }
    },
    {
        "keys": ["alt+f"],
        "command": "fold_javascript_functions",
