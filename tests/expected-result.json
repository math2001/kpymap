            {"key": "selector", "operand": "source.js", "match_all": true}
        ]
    },
    {
        "keys": ["alt+q"],
        "command": "change_quotes"
    },
    {
        "keys": ["alt+j"],
        "command": "swap_line_down"
    },
    {
        "keys": ["alt+k"],
        "command": "swap_line_up"
    },
    {
        "keys": ["alt+l"],
        "command": "indent"
    },
    {
